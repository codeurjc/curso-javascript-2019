{
    "compilerOptions": {
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "out",
        "allowJs": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
    },
    "include": ["*"],
    "exclude": ["out/**/*"]
}